<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Upcoming Movies"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <panel
                jcr:primaryType="nt:unstructured"
                jcr:title="Tab Configurations"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <main-header
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Main header text"
                                name="./header"
                                required="false"/>
                            <movie-details
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Movie Details"
                                name="./movieDetails"
                                required="false"/>
                            <experience
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Experience"
                                name="./experience"
                                required="false"/>
                            <reviews
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Movie Reviews"
                                name="./reviews"
                                required="false"/>
                            <contact-us
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Contact Us"
                                name="./contact"
                                required="false"/>
                            <random-movie
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Upcoming Movies"
                                name="./random"
                                required="false"/>
                        </items>
                    </columns>
                </items>
            </panel>
            <movie-details
                jcr:primaryType="nt:unstructured"
                jcr:title="Movie Details"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <video
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Trailer Video URL"
                                name="./video"/>
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Movie Name"
                                name="./movieName"/>
                            <release-date
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Release Date"
                                name="./releaseDate"/>
                            <directors
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Director"
                                name="./directors"/>
                            <length
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Movie Length"
                                name="./movieLength"/>
                            <production-house
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Production House"
                                name="./productionHouse"/>
                            <languge
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Language"
                                name="./language"/>
                            <note
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Special Note"
                                name="./note"/>
                            <file
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/fileupload"
                                autoStart="{Boolean}false"
                                class="cq-droptarget"
                                fieldLabel="Image asset"
                                fileNameParameter="./fileName"
                                mimeTypes="[image]"
                                multiple="{Boolean}false"
                                name="./file"
                                title="Movie image"
                                uploadUrl="${suffix.path}"
                                useHTML5="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </movie-details>
            <experience
                jcr:primaryType="nt:unstructured"
                jcr:title="Experience"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <rating
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Our Rating"
                                name="./rating">
                                <items jcr:primaryType="nt:unstructured">
                                    <one
                                        jcr:primaryType="nt:unstructured"
                                        text="One"
                                        value="one"/>
                                    <two
                                        jcr:primaryType="nt:unstructured"
                                        text="Two"
                                        value="two"/>
                                    <three
                                        jcr:primaryType="nt:unstructured"
                                        text="Three"
                                        value="three"/>
                                    <four
                                        jcr:primaryType="nt:unstructured"
                                        text="Four"
                                        value="four"/>
                                    <five
                                        jcr:primaryType="nt:unstructured"
                                        text="Five"
                                        value="five"/>
                                </items>
                            </rating>
                        </items>
                    </columns>
                </items>
            </experience>
            <reviews
                jcr:primaryType="nt:unstructured"
                jcr:title="Reviews"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <review1person
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Review 1 - Person"
                                name="./review1person"/>
                            <review1description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Review 1 - Description"
                                name="./review1description"/>
                            <review2person
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Review 2 - Person"
                                name="./review2person"/>
                            <review2description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Review 2 - Description"
                                name="./review2description"/>
                        </items>
                    </columns>
                </items>
            </reviews>
            <random-movie
                jcr:primaryType="nt:unstructured"
                jcr:title="Upcoming Movies"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <movies-path
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select the path from where banner will be displayed"
                                fieldLabel="Path to Movies"
                                name="./moviesPath"/>
                            <number
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Number of Random Movies"
                                name="./maxMovies"/>
                        </items>
                    </columns>
                </items>
            </random-movie>
            <contact-us
                jcr:primaryType="nt:unstructured"
                jcr:title="Contact Us"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Description"
                                name="./contactDescription"/>
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Contact Person"
                                name="./contactPerson"/>
                            <position
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Person Position"
                                name="./contactPosition"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Person Email"
                                name="./contactEmail"/>
                            <contact
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Contact Number"
                                name="./contactNumber"/>
                        </items>
                    </columns>
                </items>
            </contact-us>
        </items>
    </content>
</jcr:root>
